{
	"bsonType": "object",
	"required": ["title","content","day"],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"title":{
			"bsonType": "string",
			"title": "标题"
		},
		"content":{
			"bsonType": "string",
			"title": "天数"
		},
      "day":{
        "bosnType":"string",
          "title":"日期"
      }
	}
}