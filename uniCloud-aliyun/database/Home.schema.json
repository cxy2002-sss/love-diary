// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["date","input","textarea"],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"day":{
			"bsonType": "int",
			"title": "在一起的时间"
		},
		"date":{
			"bsonType": "string",
			"title": "日期"
		},
		"image":{
			"bsonType": "array",
			"title": "图片"
		},
		"input":{
			"bsonType": "string",
			"title": "标题"
		},
		"textarea":{
			"bsonType": "string",
			"title": "内容"
		}
	}
}